#!/bin/sh
set -e
if test "$CONFIGURATION" = "Debug"; then :
  cd /Users/luk14172/Projects/lerc/build/macOS
  /opt/homebrew/Cellar/cmake/3.30.5/bin/cmake -E cmake_symlink_library /Users/luk14172/Projects/lerc/build/macOS/Debug/libLerc.4.dylib /Users/luk14172/Projects/lerc/build/macOS/Debug/libLerc.4.dylib /Users/luk14172/Projects/lerc/build/macOS/Debug/libLerc.dylib
fi
if test "$CONFIGURATION" = "Release"; then :
  cd /Users/luk14172/Projects/lerc/build/macOS
  /opt/homebrew/Cellar/cmake/3.30.5/bin/cmake -E cmake_symlink_library /Users/luk14172/Projects/lerc/build/macOS/Release/libLerc.4.dylib /Users/luk14172/Projects/lerc/build/macOS/Release/libLerc.4.dylib /Users/luk14172/Projects/lerc/build/macOS/Release/libLerc.dylib
fi
if test "$CONFIGURATION" = "MinSizeRel"; then :
  cd /Users/luk14172/Projects/lerc/build/macOS
  /opt/homebrew/Cellar/cmake/3.30.5/bin/cmake -E cmake_symlink_library /Users/luk14172/Projects/lerc/build/macOS/MinSizeRel/libLerc.4.dylib /Users/luk14172/Projects/lerc/build/macOS/MinSizeRel/libLerc.4.dylib /Users/luk14172/Projects/lerc/build/macOS/MinSizeRel/libLerc.dylib
fi
if test "$CONFIGURATION" = "RelWithDebInfo"; then :
  cd /Users/luk14172/Projects/lerc/build/macOS
  /opt/homebrew/Cellar/cmake/3.30.5/bin/cmake -E cmake_symlink_library /Users/luk14172/Projects/lerc/build/macOS/RelWithDebInfo/libLerc.4.dylib /Users/luk14172/Projects/lerc/build/macOS/RelWithDebInfo/libLerc.4.dylib /Users/luk14172/Projects/lerc/build/macOS/RelWithDebInfo/libLerc.dylib
fi

